{
    "name": "yiisoft/yii-jquery",
    "type": "library",
    "description": "Yii Framework jQuery Extension",
    "keywords": ["yii", "jquery", "javascript", "client script"],
    "license": "BSD-3-Clause",
    "support": {
        "source": "https://github.com/yiisoft/yii-jquery",
        "issues": "https://github.com/yiisoft/yii-jquery/issues",
        "forum": "http://www.yiiframework.com/forum/",
        "wiki": "http://www.yiiframework.com/wiki/",
        "irc": "irc://irc.freenode.net/yii"
    },
    "authors": [
        {
            "name": "Qiang Xue",
            "email": "qiang.xue@gmail.com"
        },
        {
            "name": "Alexander Makarov",
            "email": "sam@rmcreative.ru",
            "homepage": "http://rmcreative.ru/"
        },
        {
            "name": "Paul Klimov",
            "email": "klimov.paul@gmail.com"
        },
        {
            "name": "Dmitry Naumenko",
            "email": "d.naumenko.a@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "yiisoft/yii-core": "^3.0@dev"
    },
    "require-dev": {
        "foxy/foxy": "^1.0",
        "phpunit/phpunit": "^7.3",
        "hiqdev/composer-config-plugin": "^1.0@dev",
        "yiisoft/di": "^3.0@dev",
        "yiisoft/log": "^3.0@dev",
        "yiisoft/yii-dataview": "^3.0@dev",
        "yiisoft/cache": "^3.0@dev",
        "yiisoft/yii-web": "^3.0@dev"
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    ],
    "autoload": {
        "psr-4": { "Yiisoft\\Yii\JQuery\\": "src" }
    },
    "autoload-dev": {
        "psr-4": {"Yiisoft\\Yii\JQuery\\Tests\\": "tests"}
    },
    "extra": {
        "branch-alias": {
            "dev-master": "3.0.x-dev"
        },
        "config-plugin": {
            "tests": "config/tests.php"
        }
    }
}
