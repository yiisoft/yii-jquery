{
    "name": "yiisoft/yii-jquery",
    "type": "library",
    "description": "Yii Framework jQuery Extension",
    "keywords": ["yii", "jquery", "javascript", "client script"],
    "license": "BSD-3-Clause",
    "support": {
        "source": "https://github.com/yiisoft/yii-jquery",
        "issues": "https://github.com/yiisoft/yii-jquery/issues",
        "forum": "http://www.yiiframework.com/forum/",
        "wiki": "http://www.yiiframework.com/wiki/",
        "irc": "irc://irc.freenode.net/yii"
    },
    "authors": [
        {
            "name": "Qiang Xue",
            "email": "qiang.xue@gmail.com"
        },
        {
            "name": "Alexander Makarov",
            "email": "sam@rmcreative.ru",
            "homepage": "http://rmcreative.ru/"
        },
        {
            "name": "Paul Klimov",
            "email": "klimov.paul@gmail.com"
        },
        {
            "name": "Dmitry Naumenko",
            "email": "d.naumenko.a@gmail.com"
        }
    ],
    "require": {
        "yiisoft/core": "^3.0@dev",
        "npm-asset/jquery": "3.2.*@stable | 3.1.*@stable | 2.2.*@stable | 2.1.*@stable | 1.11.*@stable | 1.12.*@stable",
        "npm-asset/punycode": "1.3.*"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.3",
        "yiisoft/di": "^3.0@dev",
        "yiisoft/log": "^3.0@dev",
        "yiisoft/cache": "^3.0@dev"
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    ],
    "autoload": {
        "psr-4": { "yii\\jquery\\": "src" }
    },
    "autoload-dev": {
        "psr-4": {"yiiunit\\jquery\\": "tests"}
    },
    "extra": {
        "branch-alias": {
            "dev-master": "3.0.x-dev"
        }
    }
}
